	<queryString language="SQL">
		<![CDATA[Select  u.name AS Name, u.surname AS Surname,  strftime('%d-%m-%Y', date(pwh.date/1000, 'unixepoch'), '+1 day') AS Date, p.name AS Project, time(pwh.work_hours, 'unixepoch') AS Worked
